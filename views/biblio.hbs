<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
<main>
    {{#each biblio }}
        <h2>{{@key}}</h2>
        {{#each this }}
            <p>
                <strong>{{ author}}</strong>,
                <i>{{ title }}</i><!--
                -->{{#if owner }}<span>, {{ owner }}</span>{{/if}}<!--
                -->.
                <span>Publié{{#if feminin}}e{{/if}} le <strong>{{ pubDate }}</strong>, consulté{{#if feminin}}e{{/if}} le <strong>{{ consultDate }}</strong>.</span>
                <br>
                <a href="{{ url}}">{{ url }}</a>
            </p>
        {{/each}}
    {{/each}}
</main>

</body>
</html>
